#3rd question answer
def generate_pascals_triangle(numRows):
    triangle = []

    for i in range(numRows):
        row = [1] * (i + 1)
        for j in range(1, i):
            row[j] = triangle[i - 1][j - 1] + triangle[i - 1][j]

        triangle.append(row)

    return triangle


numRows_1 = 5
numRows_2 = 1

output_1 = generate_pascals_triangle(numRows_1)

print(f"Input: numRows = {numRows_1}")
